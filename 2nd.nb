Cell[BoxData[{
 RowBox[{
  RowBox[{"secant", "[", 
   RowBox[{"f_", ",", "ao_", ",", "bo_", ",", "n_"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "}"}], ",", 
    RowBox[{
     RowBox[{"p0", "=", 
      RowBox[{"N", "[", "ao", "]"}]}], ";", 
     RowBox[{"p1", "=", 
      RowBox[{"N", "[", "bo", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"f", "[", "p0", "]"}], "*", 
         RowBox[{"f", "[", "p1", "]"}]}], ">", "0"}], ",", 
       RowBox[{
        RowBox[{"Print", "[", "\"\<Secant method cannot applied\>\"", "]"}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"Return", "[", "]"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"i", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"i", "\[LessEqual]", "n"}], ",", 
       RowBox[{
        RowBox[{"p2", "=", 
         RowBox[{"N", "[", 
          RowBox[{"p1", "-", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"(", 
              RowBox[{"p1", "-", "p0"}], ")"}], "*", 
             RowBox[{
              RowBox[{"f", "[", "p1", "]"}], "/", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{"f", "[", "p1", "]"}], "-", 
                RowBox[{"f", "[", "p0", "]"}]}], ")"}]}]}], ")"}]}], "]"}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"Print", "[", 
         RowBox[{
         "i", ",", "\"\<  \>\"", ",", "p0", ",", "\"\<  \>\"", ",", "p1"}], 
         "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"i", "++"}], ";", "\[IndentingNewLine]", 
        RowBox[{"p0", "=", "p1"}], ";", "\[IndentingNewLine]", 
        RowBox[{"p1", "=", "p2"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<Roots=\>\"", ",", "p2"}], "]"}]}]}], "]"}]}], "\n", 
